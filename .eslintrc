{
    "ignorePatterns": ["dist"],
    "rules": {
        "indent": [
            2,
            4
        ],
        "complexity": 1,
        "no-alert": 0,
        "consistent-return": 0,
        "max-len": 0,
        "prefer-rest-params": 0,
        "class-methods-use-this": 0,
        "func-names": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint"
}
